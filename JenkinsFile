pipeline {

	agent any
         tools{
           maven 'm 3.9.5'
             }
	stages{   
		stage(clean){
			steps{
				    sh 'mvn clean'
			}
			
		}
		
		stage(install){
			steps{
				    sh 'mvn install'
			     
			}
			post{
			    always{
			        archiveArtifacts artifacts: 'target/**.jar', followSymlinks: false
			    }
			}
		}
	    }
	}


